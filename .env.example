# Environment Variables for Onchain Test Kit
# Copy this file to .env and fill in your values
# NEVER commit the .env file to version control

# Test wallet seed phrase (NEVER use real funds - test phrases only!)
E2E_TEST_SEED_PHRASE="your test seed phrase here"

# Optional: RPC URLs for fork mode testing
# Get API keys from providers like Alchemy, Infura, etc.
ETHEREUM_RPC_URL="https://eth-mainnet.g.alchemy.com/v2/your-api-key"
BASE_RPC_URL="https://mainnet.base.org"

# Add other environment variables as needed for your tests